{
  "name": "cdk-workshop",
  "version": "0.1.0",
  "bin": {
    "cdk-workshop": "bin/cdk-workshop.js"
  },
  "scripts": {
    "deploy": "npm run update-stack && npm run publish-frontend && npm run publish-backend",
    "update-stack": "(cd ../backend && npm run build) && cdk deploy --require-approval=never",
    "publish-frontend": "(cd ../frontend && npm run build) && ts-node src/publish-frontend.ts",
    "publish-backend": "(cd ../backend && npm run build) && ts-node src/publish-backend.ts"
  },
  "devDependencies": {
    "@types/node": "8.10.45",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.33.0",
    "@aws-cdk/aws-cloudfront": "^1.33.0",
    "@aws-cdk/aws-codebuild": "^1.33.0",
    "@aws-cdk/aws-cognito": "^1.35.0",
    "@aws-cdk/aws-dynamodb": "^1.33.0",
    "@aws-cdk/aws-iam": "^1.33.0",
    "@aws-cdk/aws-lambda": "^1.33.0",
    "@aws-cdk/aws-s3": "^1.33.0",
    "@aws-cdk/aws-s3-deployment": "^1.35.0",
    "@aws-cdk/aws-sns": "^1.33.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.33.0",
    "@aws-cdk/aws-sqs": "^1.33.0",
    "@aws-cdk/core": "^1.33.0",
    "aws-cdk": "^1.33.0",
    "aws-sdk": "^2.665.0",
    "child_process": "^1.0.2",
    "jwk-to-pem": "^2.0.3",
    "module": "^1.2.5"
  }
}
