{
  "name": "stak",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "foo": "echo hello",
    "postinstall": "npm-run-all install-infra install-ui install-api",
    "install-infra": "npm install --prefix=infra",
    "install-ui": "npm install --prefix=ui",
    "install-api": "npm install --prefix=api",
    "start": "npm-run-all --parallel start-ui start-api",
    "start-ui": "npm run start --prefix=ui",
    "start-api": "npm run start --prefix=api",
    "debug": "npm-run-all --parallel start-ui debug-api",
    "debug-api": "npm run debug --prefix=api",
    "build": "npm-run-all build-ui build-api",
    "build-ui": "npm run build --prefix=ui",
    "build-api": "npm run build --prefix=api",
    "deploy": "npm-run-all build-api deploy-infra deploy-ui deploy-api",
    "deploy-ui": "npm run deploy-ui --prefix=infra",
    "deploy-api": "npm run deploy-api --prefix=infra",
    "deploy-infra": "npm run deploy-infra --prefix=infra",
    "use-datastore": "npm run use-datastore --prefix=infra",
    "test-stak-scripts": "ts-node infra/test/test-scripts.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martinpllu/stak.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
